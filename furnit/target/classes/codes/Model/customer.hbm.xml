<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="codes.Model.Cust" table="CUSTOMER">
        <id column="ID" name="id" type="java.lang.Integer" >
        <generator class="increment"/>
        </id>
        <property column="NAME" name="userName" type="java.lang.String" />
        
        <property column="EMAIL" name="email" type="java.lang.String" />
        <property column="Telephone" name="phone" type="java.lang.String" />
        <property column="MSG" name="msg" type="java.lang.String" />
    </class>
</hibernate-mapping>
